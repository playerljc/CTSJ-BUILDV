    "crypto-js": "^4.1.1",
    "eslint": "^8.9.0",
    "eslint-plugin-comments": "^1.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-vue": "^8.4.1",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
