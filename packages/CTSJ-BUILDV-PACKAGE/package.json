{
  "name": "@ctsj/buildv-package",
  "version": "1.0.0",
  "private": true,
  "description": "package",
  "main": "lib/index.js",
  "files": [
    "lib",
    "umd"
  ],
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildpackage": "ctbuildv buildpackage --packagename index --define alias=@,evnVars=true",
    "buildumd": "ctbuildv buildumd --packagename Package --define alias=@,evnVars=true",
    "generatorchangelog": "bd changelog/CHANGELOG.html",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "link": "npm link @ctsj/buildv"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playerljc/CTSJ-VuexGenerator.git"
  },
  "resolutions": {
    "graceful-fs": "4.2.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/playerljc/CTSJ-VuexGenerator/issues"
  },
  "homepage": "https://github.com/playerljc/CTSJ-VuexGenerator#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/runtime": "^7.7.4",
    "@umijs/fabric": "^2.5.13",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.9.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0"
  },
  "dependencies": {
    "ant-design-vue": "^1.7.5",
    "moment": "^2.29.1",
    "vue": "^2.6.12"
  }
}
