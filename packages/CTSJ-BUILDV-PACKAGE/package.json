{
  "name": "@ctsj/buildv-package",
  "version": "5.0.1",
  "private": true,
  "description": "package",
  "main": "lib/index.js",
  "files": [
    "lib",
    "umd"
  ],
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildpackagetswp": "ctbuildv buildpackagetswp --packagename index --define alias=@,evnVars=true",
    "buildpackagets": "ctbuildv buildpackagets",
    "buildpackage": "ctbuildv buildpackage",
    "buildumd": "ctbuildv buildumd --config ctbuildv.config.umd.js --packagename Package --define alias=@,evnVars=true",
    "generatorchangelog": "bd changelog/CHANGELOG.html",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "update": "yarn upgrade",
    "link": "npm link @ctsj/buildv"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playerljc/CTSJ-VuexGenerator.git"
  },
  "resolutions": {
    "graceful-fs": "4.2.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/playerljc/CTSJ-VuexGenerator/issues"
  },
  "homepage": "https://github.com/playerljc/CTSJ-VuexGenerator#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/runtime": "^7.17.2",
    "@umijs/fabric": "^2.10.1",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^8.9.0",
    "eslint-plugin-comments": "^1.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-vue": "^8.4.1",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "ant-design-vue": "^3.0.0-beta.9",
    "moment": "^2.29.1",
    "vue": "^3.2.31"
  }
}
